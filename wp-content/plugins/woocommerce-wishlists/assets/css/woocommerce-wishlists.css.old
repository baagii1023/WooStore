.modal {position: fixed;top: 50%;left: 50%;z-index: 9999;width: 560px;margin: -250px 0 0 -280px;background-color: #ffffff;border: 1px solid #999;border: 1px solid rgba(0, 0, 0, 0.3);*border: 1px solid #999;
/* IE6-7 */
